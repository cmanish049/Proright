.main-box{margin-bottom:30px}
		.main-box .box-header-container{
			background-color: #e5e5e5;
			padding:3px 0;
			border:1px solid #d9d9d9;
			margin-bottom:20px;	
			width:245px;	
			position:relative;			
		}
		
		.main-box .box-header-container .caret{position:absolute;bottom:-8px;left:50%;
			margin-left:-8px;
			
			border-left: 8px solid transparent;
			border-right: 8px solid transparent;
			border-top: 8px solid #e5e5e5;
		}
		
		.main-box .box-header{color:#ad1212;margin:0;text-align:center;font-size:12px;line-height:22px;}
		.main-box .box-content{padding:0px;}
		
		.btn-group > .btn-user-menu{	
			background-color: #e5e5e5;color:#000;font-size:12px;border:1px solid #d9d9d9;background-image:none;}
		.btn-group > .btn-user-menu .caret{
			border-left: 8px solid transparent;
			border-right: 8px solid transparent;
			border-top: 8px solid #b74b4b;
			margin-top:5px;
		}
		
		#menu-container{background-color:tranparent;padding:0px 0;margin-right:10px}
		#menu{background:url('images/menu-bg-2.png') repeat-x scroll 0 0 transparent;
		height:84px;width:780px}
		
		#menu-ul{padding-top:0px;margin:0}
		#menu-ul li{text-align:center;width:120px;margin-right:0px;}
		#menu-ul li a{position:relative;padding: 5px 0px 7px 0;color:#fff;text-shadow:0px 1px 1px #141414;}
		#menu-ul li a span{
			-webkit-transition: color 0.6s ease;
			-moz-transition: color 0.6s ease;
			-ms-transition: color 0.6s ease;
			-o-transition: color 0.6s ease;
			transition: color 0.6s ease;
		}

		#menu-ul li a:hover{color:#000;text-decoration:none;text-shadow:none;
			background:url("images/menu-blur.png") no-repeat scroll -8px 20px transparent;
		}
		#menu-ul li a span{ font-size:14px; font-family:Georgia;display:block;padding-top:0px;}
		
		#menu-ul li a div{background:url('images/menu-circle.png') no-repeat scroll 0 0 transparent;
			display:inline-block;}		
		#menu-ul li a:hover div{background:url('images/menu-circle-hover.png') no-repeat scroll center 0 transparent;			}
		
		#menu-ul .divider-vertical{
			margin:0;height:70px;width:1px;margin-top:6px;
			background:url('images/menu-dividerr.png') no-repeat scroll 0 0 #4f99eb;
			-webkit-box-shadow:  0px 0px 3px 1px rgba(255,255,255,0.3);
			box-shadow:  0px 0px 1px 1px rgba(255,255,255,0.3);
			}
		.menu-blur{position:absolute;width:113px;height:73px;bottom:-5px;left:0;}	
		.popover{background-color:#e5e5e5;}
		.popover-title{background-color:#e5e5e5;border-color:#ccc;font-weight:bold}
		.popover.right .arrow {border-right-color: #E5E5E5;}
		.popover.right .arrow:after {border-right-color: transparent;}




		
.left-sidebar-menu{}
.left-sidebar-menu li{/*border:5px solid #333333;*/margin-bottom:5px}
.left-sidebar-menu li a{color:#ad1212;padding:5px 5px 5px 5px;display:block;position:relative;height:25px;background-color:#333;
	-webkit-border-radius: 5px;border-radius: 5px;
}
.left-sidebar-menu .text{
	line-height:25px;;font-family:Georgia;font-size:13px;
	text-align:center;display:block;padding:0 0 0 0px;background-color:#fff;
}
.left-sidebar-menu .arrow{	
	position:absolute;
	top:0px; left:0px;
	/*width:39px;*/ width:0;height:35px;
	background:url('images/sidebar-left-menu-arrow.png') no-repeat scroll 0 0 transparent;
}
.left-sidebar-menu .icon{	
	position:absolute;
	left:0;top:3px;
	/*width:35px;*/ width:0;height:27px;
}		
		

/* Example One */
#example-one { 	
	position: relative; 
	border-bottom:1px solid transparent;
}

#example-one > li > a{
	font-size:12px; color:#333;font-family: arial;
	padding-bottom: 2px;
}
#example-one > .active > a, #example-one > .active > a:hover{
	border:1px solid transparent;
	background:none;
}

#example-one > li > a:hover, #example-one > li > a:hover{
	border:1px solid transparent;
	background:none;
}

.magic-line-container{background-color:#e5e5e5;height:9px;}
#magic-line { 
	position: absolute;
	bottom: 1px; 
	left: 0; 
	width: 100px; 
	height: 5px; 
	background: #ad1212;
	-webkit-border-radius: 50px 50px 50px 50px;
        border-radius: 50px 50px 50px 50px;
}

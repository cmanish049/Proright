.page-header-container{padding: 0 10px}
.page-header{}


table{max-width:none;}
.help-block, .help-inline {color: #999999;font-size: 11px;}   
.tooltip{z-index: 1032;}

body{background:none repeat 0 0 #EAEAEC;font-size: 13px;}
.iframe-body{padding: 0;background-color: transparent;}
.ajax-layout{padding: 10px 0 0 0;}

.clearfix:before,.clearfix:after{content:'.';display:block;overflow:hidden;visibility:hidden;font-size:0;line-height:0;width:0;height:0;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}

.white-background{background-color:#FFF;}
.section{padding:20px 0px 10px 0px;}
.section-padding{padding:10px;}
.section-left-padding{padding-left:10px;}
.section-right-padding{padding-right:10px;}
.section-top-padding{padding-top:10px;}

.center-inner{text-align: center;}
.portlet .accordion-heading h3 a,.portlet .accordion-heading h4 a{color:#333;}
.ico-mug{background:url(img/ico_mug.jpg) no-repeat 5px center #f1f1f1;padding-left:40px;margin-bottom:20px;}
    .ico-mug a{margin-left:10px;position:relative;top:-5px;}

.grid-row-action-menu{text-align: center;}
.grid-row-action-menu .basic-tooltip span{display: none;}
    
.form-lbl{font-weight:bold;}
.input-date{width:70px;}
.input-time{width:40px;}
.tooltip-resim{cursor:pointer;}
.child-list{margin-left:50px;list-style: none;}
/*buttons*/
.btn-autocomplete-new{margin-left: 5px;vertical-align: middle;padding-top: 5px;}
.btn-autocomplete-new:hover{text-decoration: none;}
.btn-autocomplete-new span{display: none}
/*form*/
.form-actions{background-color:transparent;}
/*image-div*/
.image-div{*zoom:1;}.image-div:before,.image-div:after{display:table;content:"";}
.image-div:after{clear:both;}
.image-div{margin: 0 0 10px 0;padding: 10px 10px;position: relative;color:#222;} 
    .image-div .image{position:relative;} 
       .image-div .image img{position:relative;background:none repeat scroll 0 0 transparent;padding: 2px;border: 1px solid #ccc;margin: 0 0 0 0;} 
    .image-div .image-div-content{position: relative;} 
        .image-div .image-div-content .image-div-content-row{margin: 0 0 0 0;padding: 0 0 4px 0;position: relative;}

/*grid*/
.grid-container{width: 100%;overflow-x: auto;overflow-y: auto;border-right: 1px solid #C5C5C5;}
.action-reset-grid span{display: none;}
.k-grid-toolbar .btn-toolbar{padding: 0;margin: 0;}
.quickview-grid thead th.label{width:250px;}
.quickview-grid thead th.operator{width:10px;}
/*box*/
.box,.box-header-container{*zoom:1;}
.box:before,.box-header-container:before,
	.box:after,.box-header-container:after{display:table;content:"";}
.box:after,.box-header-container:after{clear:both;}
.box
    {margin:0 0 20px 0;
    background: none repeat scroll 0 0 #FFFFFF;    
    -webkit-border-radius: 3px 3px 3px 3px;
    border-radius: 3px 3px 3px 3px;
    }    
    
    .box .box-header-container{
        *zoom: 1;
        color: #595959;		
        padding: 3px 8px 3px 15px;
		border-width: 1px 1px 1px 1px;
        border-style: solid;
        background-color: #f5f5f5;
		-webkit-border-radius: 3px 3px 0px 0px;
        border-radius: 3px 3px 0px 0px;
		
        background-image: -moz-linear-gradient(top, #fafafa, #ededed);
        background-image: -ms-linear-gradient(top, #fafafa, #ededed);
        background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fafafa), to(#ededed));
        background-image: -webkit-linear-gradient(top, #fafafa, #ededed);
        background-image: -o-linear-gradient(top, #fafafa, #ededed);
        background-image: linear-gradient(top, #fafafa, #ededed);
        background-repeat: repeat-x;
        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fafafa', endColorstr='#ededed', GradientType=0);
    /*
background: rgb(219,219,219);
background: -moz-linear-gradient(top,  rgba(219,219,219,1) 0%, rgba(193,193,193,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(219,219,219,1)), color-stop(100%,rgba(193,193,193,1)));
background: -webkit-linear-gradient(top,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
background: -o-linear-gradient(top,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
background: -ms-linear-gradient(top,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
background: linear-gradient(to bottom,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dbdbdb', endColorstr='#c1c1c1',GradientType=0 );
*/
    }
	.box .box-header-container .box-header{color:#333;font-size:24px;float:left;margin: 0}
	
	.box .box-header-container,.box .box-content{border-color:#A6A6A6;}
    .box .box-content{
		background-color:#fff;
        border-width: 0 1px 1px 1px;
        border-style: solid;
        -webkit-border-radius: 0px 0px 3px 3px;
        border-radius: 0px 0px 3px 3px;
    }
    
.box .box-content .grid-container{}
.button-box-collapse{cursor:pointer;margin-top:12px;margin-right:5px;}

/*icons*/
.icon-grid-cell-ok{background: url(img/ok-16x16.png) no-repeat scroll 0 0 transparent;width:16px;height:16px}

/********************************************************************************************************************
 * kendoui override
 ********************************************************************************************************************/
.k-pager-numbers .k-link{line-height: 22px;}
.k-grid tr td{overflow:visible;}
.k-menu a:hover,.k-button:hover,.k-button a:hover,.k-link:hover{text-decoration: none;}
.k-link input[type="checkbox"]{vertical-align: inherit;margin-right: 3px}
div.k-grid-header .k-header{font-weight: bold}


/*silver theme*/
.k-button:hover,.k-button a:hover{color:#000;}
thead .k-header{}
.k-state-selected, 
.k-list > .k-state-selected, 
.k-list > .k-state-focused, 
.k-panelbar > .k-state-selected, 
.k-panel > .k-state-selected, 
.k-button:active, 
.k-ghost-splitbar-vertical, 
.k-ghost-splitbar-horizontal, 
.k-draghandle.k-state-selected:hover{
    color: white;
background-color: #8EBC00;
border-color: #8EBC00;
}

/********************************************************************************************************************
 * form validator
 ********************************************************************************************************************/
.formError{z-index:990; white-space: normal;}
.formError .formErrorContent{z-index:991}
.formError .formErrorArrow{z-index:996}
.formErrorInsideDialog.formError{z-index:5000}
.formErrorInsideDialog.formError .formErrorContent{z-index:5001}
.formErrorInsideDialog.formError .formErrorArrow{z-index:5006}
.inputContainer{position:relative;float:left}
.formError{position:absolute;top:300px;left:300px;display:block;cursor:pointer}
.ajaxSubmit{padding:20px;background:#55ea55;border:1px solid #999;display:none}
.formError .formErrorContent{width:100%;background:#ee0101;position:relative;color:#fff;width:150px;font-size:11px;border:2px solid #ddd;box-shadow:0 0 6px #000;-moz-box-shadow:0 0 6px #000;-webkit-box-shadow:0 0 6px #000;padding:4px 10px 4px 10px;border-radius:6px;-moz-border-radius:6px;-webkit-border-radius:6px}
.greenPopup .formErrorContent{background:#33be40}
.blackPopup .formErrorContent{background:#393939;color:#FFF}
.formError .formErrorArrow{width:15px;margin:-2px 0 0 13px;position:relative}
body[dir='rtl'] .formError .formErrorArrow,body.rtl .formError .formErrorArrow{margin:-2px 13px 0 0}
.formError .formErrorArrowBottom{box-shadow:none;-moz-box-shadow:none;-webkit-box-shadow:none;margin:0 0 0 12px;top:2px}
.formError .formErrorArrow div{border-left:2px solid #ddd;border-right:2px solid #ddd;box-shadow:0 2px 3px #444;-moz-box-shadow:0 2px 3px #444;-webkit-box-shadow:0 2px 3px #444;font-size:0;height:1px;background:#ee0101;margin:0 auto;line-height:0;font-size:0;display:block}
.formError .formErrorArrowBottom div{box-shadow:none;-moz-box-shadow:none;-webkit-box-shadow:none}
.greenPopup .formErrorArrow div{background:#33be40}
.blackPopup .formErrorArrow div{background:#393939;color:#FFF}
.formError .formErrorArrow .line10{width:15px;border:0}
.formError .formErrorArrow .line9{width:13px;border:0}
.formError .formErrorArrow .line8{width:11px}
.formError .formErrorArrow .line7{width:9px}
.formError .formErrorArrow .line6{width:7px}
.formError .formErrorArrow .line5{width:5px}
.formError .formErrorArrow .line4{width:3px}
.formError .formErrorArrow .line3{width:1px;border-left:2px solid #ddd;border-right:2px solid #ddd;border-bottom:0 solid #ddd}
.formError .formErrorArrow .line2{width:3px;border:0;background:#ddd}
.formError .formErrorArrow .line1{width:1px;border:0;background:#ddd}


.grid-overflow-container{margin:0;position: relative;overflow:auto;border:1px solid #999;padding:0;}.grid-container{word-wrap: break-word;/*border:1px solid #999;*/margin:0;position: relative;display:block;}table.datagrid{table-layout:fixed;border-bottom:1px solid #F7F7F7;border-collapse:separate;border-spacing:0;color:#444444;font-size:1.2em;margin:0;}.datagrid td,.datagrid th{font-size:11px;font-family:Tahoma, Verdana, Arial, sans-serif;border-bottom:1px solid #ABABAB;vertical-align:middle;margin:0;}table.datagrid td{text-align:left;border-color: #C9C9C9; border-style: solid; border-width: 0 1px 1px 0; padding:0px 0px 0px 0px;}table.datagrid tr{position: relative;}table.datagrid .col-container-div{padding: 5px 10px 5px 5px; display: block;position: relative;clear:both;}table.datagrid thead tr.title-row{background:url(img/bg_table_top.png) repeat-x scroll 0 0 #D0D0D0;color:#515151;font-family:verdana;font-size:12px;font-weight:bold;}table.datagrid thead tr.search-inputs-row{background:none repeat-x scroll 0 0 #ffffff;color:#515151;font-family:verdana;font-size:12px;font-weight:bold;}table.datagrid thead tr.search-inputs-row input[type="text"]{    height: 26px;    -moz-box-sizing:    border-box;     -webkit-box-sizing: border-box;    box-sizing:        border-box;    padding: 3px 3px;margin:0px;width:100%; }table.datagrid th{text-align:left;font-weight:bold;border-bottom:1px solid #C9C9C9;padding:2px 5px 2px 5px;border-right: 1px solid #c9c9c9;}/*table.datagrid tr.odd{background:url(img/bg_table_light.png) repeat-x scroll 0 0 #E3E4E5;}table.datagrid tr.even{background:url(img/bg_table_dark.png) repeat-x scroll 0 0 #E3E4E5;}table.datagrid tr.even-odd{background:url(img/even-odd.png) repeat scroll 0 0 #E3E4E5;}*/table.datagrid tr.odd{background:none repeat-x scroll 0 0 #fff;}table.datagrid tr.even{background:none repeat-x scroll 0 0 #fff;}table.datagrid tr.even-odd{background:none repeat scroll 0 0 #fff;}.grid-loading{background:url(img/ajax-loader.gif) no-repeat scroll 0 0 transparent;display:block;opacity:.50;-moz-opacity:.50;filter:alpha(opacity=50);text-align:center;position:fixed;left:45%;top:44%;width:100%;height:100%;}.grid-header-bg{background:#E3E3E3 url(img/grid_header_bg.png) repeat-x bottom;}.grid-row-style1{background-color:#FFFFFF;}.grid-row-style2{background-color:#EEF6FF;}.grid-row-style3{background-color:#EEEEEE;}.grid-row-hover{background-color:#FFFFBB !important;}.grid-row-sel{background-color:yellow !important;border-bottom:1px solid #333333;}table.datagrid td.grid-custom-col{color:#333333;background-color:red;text-align:right;}table.datagrid th div.grid-col-resize{background:url(img/grid-split.gif) no-repeat center;cursor:col-resize;width:6px;height:16px;font-size:1px;float:right;display:block;text-indent:-1000px;overflow:hidden;}.grid-sort-asc{background:url(img/grid_asc.png) no-repeat center left;display:block;height:20px;cursor:pointer;padding:3px 0 0 16px;}.grid-page-toolbar{background:url(img/grid_header_bg.png) repeat-x;font:11px Tahoma;padding-top:7px;border-bottom:1px solid #77ABF2;}.grid-page-toolbar form{float:left;margin:0;}.grid-page-toolbar form input{margin:0;padding:0px 2px;}.grid-page-start{background:url(img/grid_first.png) no-repeat center;display:block;float:left;width:16px;height:16px;text-indent:-1000px;overflow:hidden;margin:3px 3px 3px 10px;}.grid-page-prev{background:url(img/grid_prev.png) no-repeat center;display:block;float:left;width:16px;height:16px;text-indent:-1000px;overflow:hidden;margin:3px;}.grid-page-next{background:url(img/grid_next.png) no-repeat center;display:block;float:left;width:16px;height:16px;text-indent:-1000px;overflow:hidden;margin:3px;}.grid-page-end{background:url(img/grid_last.png) no-repeat center;display:block;float:left;width:16px;height:16px;text-indent:-1000px;overflow:hidden;margin:3px;}.grid-page-loading{background:url(img/grid-loading.gif) no-repeat center;display:block;float:left;width:16px;height:16px;text-indent:-1000px;overflow:hidden;cursor:pointer;margin:3px;}.grid-page-loading-done{background:url(img/grid-loading-done.gif) no-repeat center;}.grid-page-info{font:11px Tahoma;display:block;float:left;color:#575757;}.grid-page-viewing-records-info{font:11px Tahoma;color:#575757;display:block;float:left;height:16px;margin:3px 50px 0 30px;}.grid-page-input{font:11px Tahoma;width:30px;border:1px inset;}.grid-sort-desc{background:url(img/grid_desc.png) no-repeat center left;display:block;height:20px;cursor:pointer;padding:3px 0 0 16px;}.grid-sort-none{background:url(img/grid-sort-none.png) no-repeat center left;display:block;height:20px;cursor:pointer;padding:3px 0 0 16px;}/*********************************************************** * Kendim ekledim ***********************************************************/table.datagrid tr.red, table.datagrid tr.cop, table.datagrid tr.istenmeyen{background-color:#FFDBDB;}table.datagrid tr.green{background-color:#CEFFC9;}table.datagrid tr.yellow,table.datagrid tr.bekliyor,table.datagrid tr.pasif{background-color:#FFFFBB;}
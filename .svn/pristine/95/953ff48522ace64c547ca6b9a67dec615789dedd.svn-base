.tooltip{z-index: 1032;}

body{background-color:#fff;background:url('img/body-bg-wood.jpg') repeat 0 0;font-size: 13px;}
.iframe-body{background:none repeat 0 0;}
.clearfix:before,.clearfix:after{content:'.';display:block;overflow:hidden;visibility:hidden;font-size:0;line-height:0;width:0;height:0;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}

.white-background{background-color:#FFF;}
.section{padding:20px 0px 10px 0px;}
.section-padding{padding:10px;}
.section-left-padding{padding-left:10px;}
.section-right-padding{padding-right:10px;}
.section-top-padding{padding-top:10px;}

.portlet .accordion-heading h3 a,.portlet .accordion-heading h4 a{color:#333;}
.ico-mug{background:url(img/ico_mug.jpg) no-repeat 5px center #f1f1f1;padding-left:40px;margin-bottom:20px;}
    .ico-mug a{margin-left:10px;position:relative;top:-5px;}
    
.form-lbl{font-weight:normal;}
.input-date{width:70px;}
.input-time{width:40px;}
.tooltip-resim{cursor:pointer;}
.child-list{margin-left:50px;list-style: none;}
/*form*/
.form-actions{background-color:transparent;}
/*image-div*/
.image-div{*zoom:1;}.image-div:before,.image-div:after{display:table;content:"";}
.image-div:after{clear:both;}
.image-div{margin: 0 0 10px 0;padding: 10px 10px;position: relative;color:#222;} 
    .image-div .image{position:relative;} 
       .image-div .image img{position:relative;background:none repeat scroll 0 0 transparent;padding: 2px;border: 1px solid #ccc;margin: 0 0 0 0;} 
    .image-div .image-div-content{position: relative;} 
        .image-div .image-div-content .image-div-content-row{margin: 0 0 0 0;padding: 0 0 4px 0;position: relative;}

/*grid*/
.grid-container{width: 100%;overflow-x: auto;overflow-y: auto;border-right: 1px solid #C5C5C5;}

/*box*/
.box,.box-header-container{*zoom:1;}
.box:before,.box-header-container:before,
	.box:after,.box-header-container:after{display:table;content:"";}
.box:after,.box-header-container:after{clear:both;}
.box
    {margin:0 0 20px 0;
    background: none repeat scroll 0 0 #FFFFFF;    
    -webkit-border-radius: 3px 3px 3px 3px;
    border-radius: 3px 3px 3px 3px;
    }    
    
    .box .box-header-container{
        *zoom: 1;
        color: #595959;		
        padding: 3px 8px 3px 15px;
		border-width: 1px 1px 1px 1px;
        border-style: solid;
        background-color: #f5f5f5;
		-webkit-border-radius: 3px 3px 0px 0px;
        border-radius: 3px 3px 0px 0px;
		
        background-image: -moz-linear-gradient(top, #fafafa, #ededed);
        background-image: -ms-linear-gradient(top, #fafafa, #ededed);
        background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fafafa), to(#ededed));
        background-image: -webkit-linear-gradient(top, #fafafa, #ededed);
        background-image: -o-linear-gradient(top, #fafafa, #ededed);
        background-image: linear-gradient(top, #fafafa, #ededed);
        background-repeat: repeat-x;
        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fafafa', endColorstr='#ededed', GradientType=0);
    /*
background: rgb(219,219,219);
background: -moz-linear-gradient(top,  rgba(219,219,219,1) 0%, rgba(193,193,193,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(219,219,219,1)), color-stop(100%,rgba(193,193,193,1)));
background: -webkit-linear-gradient(top,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
background: -o-linear-gradient(top,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
background: -ms-linear-gradient(top,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
background: linear-gradient(to bottom,  rgba(219,219,219,1) 0%,rgba(193,193,193,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dbdbdb', endColorstr='#c1c1c1',GradientType=0 );
*/
    }
	.box .box-header-container .box-header{color:#333;font-size:24px;float:left;margin: 0}
	
	.box .box-header-container,.box .box-content{border-color:#A6A6A6;}
    .box .box-content{
		background-color:#fff;
        border-width: 0 1px 1px 1px;
        border-style: solid;
        -webkit-border-radius: 0px 0px 3px 3px;
        border-radius: 0px 0px 3px 3px;
    }
    
.box .box-content .grid-container{}
.button-box-collapse{cursor:pointer;margin-top:12px;margin-right:5px;}
